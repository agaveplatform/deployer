{% if ha_deployment %}
hap.{{ tenant_id }}.agave.tacc.utexas.edu:
    image: agaveplatform/haproxy_auth:{{ haproxy_auth_version }}
    ports:
        - "80:80"
        - "443:443"
    volumes:
        - /home/apim/auth-services/{{ tenant_id }}/{{ tenant_id }}.yml:/values.yml
        - /home/apim/auth-services/{{ tenant_id }}/passwords:/passwords
    restart: unless-stopped

{% endif %}
